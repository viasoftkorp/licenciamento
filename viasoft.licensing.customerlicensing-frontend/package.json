{
  "name": "customer-licensing",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "client": "cd ./src && node ../node_modules/@viasoft/client/src/index.v2.js",
    "serve:customer-licensing-portal": "ng serve customer-licensing-portal --port 4201 --deploy-url http://localhost:4201/",
    "build:customer-licensing-portal": "ng build customer-licensing-portal --configuration production --deployUrl=!CDN_URL/",
    "build:jenkins": "ng build customer-licensing-portal --configuration production --deployUrl=https://cdn-interno.korp.com.br/v1/customer-licensing/",
    "build:jenkins-prod": "ng build customer-licensing-portal --configuration production --deployUrl=https://cdn-connect.korp.com.br/v1/customer-licensing/",
    "jenkins:test": "node ./test.script.js",
    "jenkins:build-application": "node ./node_modules/@angular/cli/bin/ng build !APP_NAME!APP_BUILD_SUFFIX --configuration production --output-path=!APP_FOLDER --deploy-url=!BUILD_CDN_URL",
    "cypress:serve": "ng serve --configuration=mock",
    "cypress:open": "npx cypress open",
    "serve:ci": "ng serve --host=0.0.0.0 --disable-host-check",
    "serve:dev-env": "node replace-dev-env-variables.js && ng serve --configuration=dev-env"
  },
  "private": true,
  "dependencies": {
