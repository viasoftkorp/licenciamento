{
  "name": "license-management",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve license-management",
    "serve:test": "ng serve --host=0.0.0.0 --disable-host-check --configuration=mock",
    "serve:mock": "ng serve license-management --configuration=mock",
    "cypress:serve": "node ./node_modules/@angular/cli/bin/ng serve --host=0.0.0.0 --disable-host-check --configuration=mock --watch=false --live-reload=false",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "client": "node node_modules/@viasoft/client/src/index.v2.js",
    "build:single-spa": "ng build --configuration production --deploy-url http://localhost:4200/",
    "serve:single-spa": "ng serve --disable-host-check --port 4200 --deploy-url http://localhost:4200/ --live-reload false",
    "build:jenkins": "ng build license-management-portal --configuration production --deploy-url https://cdn-interno.korp.com.br/v1/license-management/",
    "build:jenkins-prod": "ng build license-management-portal --configuration production --deploy-url https://cdn-connect.korp.com.br/v1/license-management/",
    "jenkins:test": "node ./test.script.js",
    "jenkins:build-application": "node ./node_modules/@angular/cli/bin/ng build !APP_NAME!APP_BUILD_SUFFIX --configuration production --output-path !APP_FOLDER --deploy-url !BUILD_CDN_URL",
    "jenkins:build-package:licensing-licensingmanagement-licensedtenant": "npm run build:licensing-licensingmanagement-licensedtenant",
    "build:licensing-licensingmanagement-licensedtenant": "ng build licensing-licensingmanagement-licensedtenant --configuration production",
    "i:licensing-licensingmanagement-licensedtenant": "npm i .\\dist\\licensing-licensingmanagement-licensedtenant\\",
    "bi:licensing-licensingmanagement-licensedtenant": "npm run build:licensing-licensingmanagement-licensedtenant && npm run i:licensing-licensingmanagement-licensedtenant",
    "serve:dev-env": "node replace-dev-env-variables.js && ng serve --configuration=dev-env"
  },
